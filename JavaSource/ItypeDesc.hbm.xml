<hibernate-mapping package="org.unitime.timetable.model">
	<class
		name="ItypeDesc"
		table="ITYPE_DESC"
		mutable="false" >

		<cache usage="read-only" />
		
        <id
            name="itype"
            column="ITYPE"
            type="java.lang.Integer"
			length="2" >
        </id>
        
		<property
			name="abbv"
			column="ABBV"
			type="java.lang.String"
			not-null="false"
			length="7"
		/>

		<property
			name="desc"
			column="DESCRIPTION"
			type="java.lang.String"
			not-null="false"
			length="50"
		/>
		
		<property
			name="sis_ref"
			column="SIS_REF"
			type="java.lang.String"
			not-null="false"
			length="20"
		/>
		
		<property
			name="basic"
			column="BASIC"
			type="java.lang.Integer"
			not-null="false"
			length="1"
		/>

	</class>
</hibernate-mapping>