<hibernate-mapping package="org.unitime.timetable.model">
    <class name="JointEnrollment" table="jenrl">
        <cache include="non-lazy" usage="nonstrict-read-write"/>
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">jenrl_seq</param>
            </generator>
        </id>

        <property
            name="jenrl"
            column="jenrl"
            type="java.lang.Double"
            not-null="true"/>

        <many-to-one
            name="solution"
            class="Solution"
            column="solution_id"
            not-null="true"
            foreign-key="fk_jenrl_solution"/>

        <many-to-one
            name="class1"
            class="Class_"
            column="class1_id"
            not-null="true"
            foreign-key="fk_jenrl_class1"
            cascade="all"/>

        <many-to-one
            name="class2"
            class="Class_"
            column="class2_id"
            not-null="true"
            foreign-key="fk_jenrl_class2"
            cascade="all"/>

    </class>
</hibernate-mapping>
