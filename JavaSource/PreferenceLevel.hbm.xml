<hibernate-mapping package="org.unitime.timetable.model">
	<class
		name="PreferenceLevel"
		table="PREFERENCE_LEVEL"
		mutable="false">

 		<cache usage="read-only"/>

        <id
            name="uniqueId"
            column="UNIQUEID"
            type="java.lang.Long"
        >
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">PREF_LEVEL_SEQ</param>
            </generator>
        </id>

		<property
			name="prefId"
			column="PREF_ID"
			type="java.lang.Integer"
			not-null="true"
			length="2"
		/>

		<property
			name="prefProlog"
			column="PREF_PROLOG"
			type="java.lang.String"
			not-null="false"
			length="2"
		/>

		<property
			name="prefName"
			column="PREF_NAME"
			type="java.lang.String"
			not-null="false"
			length="20"
		/>

	</class>
</hibernate-mapping>