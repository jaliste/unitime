<hibernate-mapping package="org.unitime.timetable.model">
    <class name="StudentClassEnrollment" table="student_class_enrl">
        <cache include="non-lazy" usage="nonstrict-read-write"/>
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">pref_group_seq</param>
            </generator>
        </id>

        <many-to-one
            name="student"
            class="Student"
            column="student_id"
            not-null="true"
            foreign-key="fk_student_class_enrl_student"/>

        <many-to-one
            name="courseRequest"
            class="CourseRequest"
            column="course_request_id"
            foreign-key="fk_student_class_enrl_request"
            not-null="false"/>

        <many-to-one
            name="courseOffering"
            class="CourseOffering"
            column="course_offering_id"
            foreign-key="fk_student_class_enrl_course"
            not-null="false"/>

        <many-to-one
            name="clazz"
            class="Class_"
            column="class_id"
            not-null="true"
            foreign-key="fk_student_class_enrl_class"/>

        <property
            name="timestamp"
            column="timestamp"
            type="java.util.Date"
            not-null="true"/>
            
        <property
            name="approvedDate"
            column="approved_date"
            type="java.util.Date"
            not-null="false"/>

        <property
            name="approvedBy"
            column="approved_by"
            type="java.lang.String"
            length="40"
            not-null="false"/>

        <property
            name="changedBy"
            column="changed_by"
            type="java.lang.String"
            not-null="false"
            length="40"/>

    </class>
</hibernate-mapping>
