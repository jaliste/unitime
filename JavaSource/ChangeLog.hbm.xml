<hibernate-mapping package="org.unitime.timetable.model">
    <class name="ChangeLog" table="change_log">
        <cache include="non-lazy" usage="nonstrict-read-write"/>
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">pref_group_seq</param>
            </generator>
        </id>

        <many-to-one
            name="session"
            class="Session"
            column="session_id"
            not-null="true"
            foreign-key="fk_change_log_session"/>

        <many-to-one
            name="manager"
            class="TimetableManager"
            column="manager_id"
            not-null="true"
            foreign-key="fk_change_log_manager"/>

        <property
            name="timeStamp"
            column="time_stamp"
            type="java.sql.Timestamp"
            not-null="true"/>

        <property
            name="objectType"
            column="obj_type"
            type="java.lang.String"
            not-null="true"
            length="255"/>

        <property
            name="objectTitle"
            column="obj_title"
            type="java.lang.String"
            not-null="true"
            length="255"/>

        <property
            name="objectUniqueId"
            column="obj_uid"
            type="java.lang.Long"
            not-null="true"/>

        <many-to-one
            name="subjectArea"
            class="SubjectArea"
            column="subj_area_id"
            foreign-key="fk_change_log_subjarea"/>

        <many-to-one
            name="department"
            class="Department"
            column="department_id"
            foreign-key="fk_change_log_department"/>

        <property
            name="sourceString"
            column="source"
            type="java.lang.String"
            not-null="true"
            length="50"/>

        <property
            name="operationString"
            column="operation"
            type="java.lang.String"
            not-null="true"
            length="50"/>

        <property
            name="detail"
            column="detail"
            type="org.unitime.commons.hibernate.blob.XmlBlobType"
            length="65535"/>

    </class>
</hibernate-mapping>
