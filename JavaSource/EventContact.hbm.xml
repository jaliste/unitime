<hibernate-mapping package="org.unitime.timetable.model">
	<class
		name="EventContact"
		abstract="false"
		table="EVENT_CONTACT">

		<cache include="non-lazy" usage="read-write"/>

		<id 
            name="uniqueId"
            column="UNIQUEID" 
            type="java.lang.Long">            
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
				<param name="sequence">PREF_GROUP_SEQ</param>
			</generator>
            
        </id>
        
		<property
			name="externalUniqueId"
			column="EXTERNAL_ID"
			type="string"
			not-null="false"
			length="40"
		/>
		
		<property
			name="emailAddress"
			column="EMAIL"
			type="string"
			not-null="false"
			length="135"
		/>
		
		<property
			name="phone"
			column="PHONE"
			type="string"
			not-null="false"
			length="10"
		/>
				
		<property
			name="firstName"
			column="FIRSTNAME"
			type="string"
			not-null="true"
			length="20"
		/>
		
		<property
			name="middleName"
			column="MIDDLENAME"
			type="string"
			not-null="false"
			length="20"
		/>
		
		<property
			name="lastName"
			column="LASTNAME"
			type="string"
			not-null="true"
			length="30"
		/>
		
	</class>
</hibernate-mapping>