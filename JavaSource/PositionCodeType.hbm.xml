<hibernate-mapping package="org.unitime.timetable.model">
    <class name="PositionCodeType" table="position_code_to_type">
        <cache include="non-lazy" usage="read-write"/>
        <id
            name="positionCode"
            column="position_code"
            type="java.lang.String"
            length="5"/>

        <many-to-one
            name="positionType"
            column="pos_code_type"
            class="PositionType"
            foreign-key="fk_pos_map_type"/>

        <set
            name="staff"
            lazy="true"
            table="staff">
            <cache include="non-lazy" usage="read-write"/>
            <key column="pos_code"/>
            <one-to-many class="Staff"/>
        </set>

    </class>
</hibernate-mapping>
