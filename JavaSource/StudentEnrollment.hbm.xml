<hibernate-mapping package="org.unitime.timetable.model">
	<class
		name="StudentEnrollment"
		table="STUDENT_ENRL"
		mutable="false">
		
 		<cache include="non-lazy" usage="nonstrict-read-write"/>

        <id
            name="uniqueId"
            column="UNIQUEID"
            type="java.lang.Long"
        >
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">STUDENT_ENRL_SEQ</param>
            </generator>
        </id>
		
		<property
			name="studentId"
			column="STUDENT_ID"
			type="java.lang.Long"
			not-null="true"
			length="20"
		/>

		<many-to-one 
			name="solution" 
			class="Solution" 
			column="SOLUTION_ID" 
			not-null="true"
			foreign-key="FK_STUDENT_ENRL_SOLUTION"
		/>

		<many-to-one 
			name="clazz" 
			class="Class_" 
			column="CLASS_ID" 
			not-null="true"
			foreign-key="FK_STUDENT_ENRL_CLASS"
		/>
	</class>
</hibernate-mapping>