<hibernate-mapping package="org.unitime.timetable.model">
	<class name="RoomTypeOption" table="ROOM_TYPE_OPTION">
		
 		<cache include="non-lazy" usage="nonstrict-read-write"/>
 		
 		<composite-id>
 			<key-many-to-one 
 				name="roomType" 
 				class="RoomType" 
 				column="ROOM_TYPE" 
 				foreign-key="fk_rtype_option_type"
 			/>
 			<key-many-to-one 
 				name="session" 
 				class="Session" 
 				column="SESSION_ID"
 				foreign-key="fk_rtype_option_session"/>
 		</composite-id>

		<property
			name="status"
			column="STATUS"
			type="java.lang.Integer"
			not-null="true"
			length="10"
		/>			
					
		<property
			name="message"
			column="MESSAGE"
			type="java.lang.String"
			not-null="true"
			length="200"
		/>
	</class>		
</hibernate-mapping>