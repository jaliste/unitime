<hibernate-mapping package="org.unitime.timetable.model">

	<class name="ExactTimeMins" table="EXACT_TIME_MINS">
		
 		<cache include="non-lazy" usage="nonstrict-read-write"/>
 		
        <id name="uniqueId" column="UNIQUEID" type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">PREF_GROUP_SEQ</param>
            </generator>
        </id>

		<property
			name="minsPerMtgMin"
			column="MINS_MIN"
			type="java.lang.Integer"
			not-null="true"
			length="4"
		/>

		<property
			name="minsPerMtgMax"
			column="MINS_MAX"
			type="java.lang.Integer"
			not-null="true"
			length="4"
		/>

		<property
			name="nrSlots"
			column="NR_SLOTS"
			type="java.lang.Integer"
			not-null="true"
			length="4"
		/>

		<property
			name="breakTime"
			column="BREAK_TIME"
			type="java.lang.Integer"
			not-null="true"
			length="3"
		/>

    </class>

</hibernate-mapping>
