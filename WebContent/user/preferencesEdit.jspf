							</html:option>
						<% } %>
					   	</logic:iterate>
					</html:select> &nbsp;
					<html:submit property="op" 
						styleClass="btn"
						title="<%=MSG.titleRemoveBuildingPreference() %>"
						onclick="<%= \"javascript: doDel('bldgPref', '\" + ctr + \"');\"%>">
						<loc:message name="actionRemoveBuildingPreference" />
					</html:submit> 			
				</TD>
			</TR>
	   	</logic:iterate>
