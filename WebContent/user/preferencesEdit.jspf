					</html:select>
					&nbsp;&nbsp;
					<html:select property='<%= "roomPrefLevels[" + ctr + "]" %>' disabled="<%=disabled%>">					
						<html:option value="-">-</html:option>
						<logic:iterate scope="request" name="<%=PreferenceLevel.PREF_LEVEL_ATTR_NAME%>" id="prLevel">
						<% pr = (PreferenceLevel)prLevel; 
